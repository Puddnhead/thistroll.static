{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "arrow-spacing": 2,
    "complexity": 2,
    "consistent-return": 2,
    "global-require": 2,
    "guard-for-in": 2,
    "max-depth": 1,
    "no-alert": 2,
    "no-debugger": 1,
    "no-empty-function": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 1,
    "no-extra-parens": 2,
    "no-floating-decimal": 1,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 1,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 2,
    "no-new": 0,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 1,
    "no-new-wrappers": 1,
    "no-param-reassign": 1,
    "no-path-concat": 1,
    "no-proto": 2,
    "no-restricted-globals": 2,
    "no-restricted-modules": 1,
    "no-restricted-properties": 1,
    "no-return-assign": 2,
    "no-return-await": 2,
    "no-self-compare": 2,
    "no-throw-literal": 2,
    "no-unused-vars": 2,
    "no-undef": 1,
    "no-undefined": 2,
    "no-use-before-define": 1,
    "no-useless-call": 2,
    "no-useless-constructor": 1,
    "no-useless-return": 2,
    "no-void": 2,
    "quotes": 2,
    "space-before-blocks": 2,
    "vars-on-top": 1
  }
}
